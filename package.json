{
  "name": "@yofriadi/tanitech-utils",
  "description": "TaniHub package utility",
  "version": "0.1.0",
  "license": "MIT",
  "repository": "git://github.com/yofriadi/tanitech-utils.git",
  "engines": {
    "node": "^10.16.3",
    "npm": "^6.9.0",
    "yarn": "^1.17.3"
  },
  "scripts": {
    "lint": "eslint . --ext js",
    "lint:fix": "eslint . --fix --ext js",
    "prettier": "prettier --write *.js --only-changed"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js)": [
      "yarn lint",
      "yarn prettier"
    ]
  },
  "eslintConfig": {
    "env": {
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "rules": {}
  },
  "eslintIgnore": [
    "node_modules"
  ],
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "dependencies": {
    "lodash": "^4.17.19"
  },
  "devDependencies": {
    "eslint": "^6.4.0",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2"
  }
}
